Merge Sort-----------
 
 		
 			[16, 21, 11,  8, 12, 22]
 		
 1)		[16, 21, 11]	[ 8, 12, 22]
 		
 2)		[16, 21] [11]	[ 8, 12] [22]
 		
 3)		[16] [21] [11]	[8] [12] [22]
 
 4)		[16, 21]  [11]	[ 8, 12] [22]
 
 5)		[11, 16, 21]	[ 8, 12, 22]		
 		
 		
 6)		[ 8, 11,   ,   ,   ,   ]
 
 					11 > 8
 
 		[  , 16, 21]	[  , 12, 22]
 		
 7)		[ 8, 11, 12, 16,   ,   ]
 
 					16 > 12
 
 		[  ,  , 21]		[  ,  , 22]
 		
 8)		[ 8, 11, 12, 16, 21, 22]
 
 					21 > 22
 
 		[  ,  , ]		[  ,  , ]
 		
 Final: [ 8, 11, 12, 16, 21, 22]
 
 
 Big O Notation: O( nlog n)
 
 
